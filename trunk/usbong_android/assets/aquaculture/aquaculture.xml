<?xml version="1.0" encoding="UTF-8"?>

<process-definition lang="Filipino" xmlns=""  name="aquaculture">


	<start-state name="start-state1">
		<transition to="Nagkaroon ba ng insidente ng fishkill sa inyong lugar?"></transition>
	</start-state>


	<task-node name="checkList~1~1~Ano ang ginawa mo ngayon?">
		<task name="Nagpakain ako ng isda."></task>
		<task name="Naglinis ako ng kulungan ng isda."></task>
		<task name="Nag-ani ako ng isda."></task>
		<transition to="audioRecord~2~Isalaysay ang nangyari." name="Any"></transition>
	</task-node>

	<task-node name="photoCapture~Kumuha ng litrato ng iyong mga isda.">
		<transition to="end-state1" name="Any"></transition>
	</task-node>

	<task-node name="audioRecord~2~Isalaysay ang nangyari.">
		<transition to="photoCapture~Kumuha ng litrato ng iyong mga isda." name="Any"></transition>
	</task-node>

	<decision name="Nagkaroon ba ng insidente ng fishkill sa inyong lugar?">
		<transition to="checkList~1~1~Ano ang ginawa mo ngayon?" name="Hindi"></transition>
		<transition to="textField~Saan?" name="Oo"></transition>
	</decision>

	<task-node name="textField~Saan?">
		<transition to="textField~Kailan pa?" name="Any"></transition>
	</task-node>

	<task-node name="textField~Kailan pa?">
		<transition to="audioRecord~1~Isalaysay ang nangyari." name="Any"></transition>
	</task-node>

	<task-node name="audioRecord~1~Isalaysay ang nangyari.">
		<transition to="UMAKSYON" name="Any"></transition>
	</task-node>

	<task-node name="UMAKSYON">
		<task name="Agad na ipagbigay alam sa mga kinauukulan ang nangyari."></task>
		<task name="Agad na ipadala ang report na ito papunta sa Ateneo gamit ang SMS o text."></task>
		<transition to="end-state1" name="Any"></transition>
	</task-node>

	<end-state name="end-state1"></end-state>
</process-definition>